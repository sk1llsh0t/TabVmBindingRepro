<?xml version="1.0" encoding="utf-8"?>

<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewModels="clr-namespace:TabVmBindingRepro.ViewModels"
             x:DataType="viewModels:MainViewModel"
             x:Class="TabVmBindingRepro.ContentViews.TabBView">
    <VerticalStackLayout HorizontalOptions="Center" Padding="20">
        <Label Text="{Binding TabBTitle}" />
        <Button Text="Tab B Button" Command="{Binding TabBButtonCommand}" />
    </VerticalStackLayout>
</ContentView>